<div class="quote-card">
  <div class="quote-body">
    <h3>"<%= quote.body %>"</h3>
  </div>
  <div class="quote-footer">
    <div id="like">
      <div class="button-holder">
        <% if ( quote ) { %>
          <img src="images/liked.png" class="heart" alt="" data="<%= quote._id%>">
        <% } else { %>
          <img src="images/unliked.png" class="heart" alt="" data="<%= quote._id%>">
        <% }; %>
      </div>
    </div>
    <div class="quote-info">
      <p><%= quote.likeCount %> Likes</p>
      <p><em><%= quote.from %> (<%= quote.location %>)</em></p>
    </div>
  </div>
</div>